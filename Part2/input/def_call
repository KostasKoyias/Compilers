name() {
    "John"
}

repeat(x) {
    x + x
}

cond_repeat(c, x) {
    if (name() + c prefix "yes" + c)
        if("yes" prefix c)
            repeat(x)
        else
            x + "x in quotes"
    else
        x
}

cond_repeat("yes", name())
cond_repeat("no", "Jane")
cond_repeat("ye", "Tarzan")
